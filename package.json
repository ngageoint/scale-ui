{
  "name": "scale-ui",
  "version": "0.17.0",
  "homepage": "http://ngageoint.github.io/scale-ui",
  "scripts": {
    "ng": "ng",
    "api": "nodemon --watch api api/ApiServer.js",
    "release": "standard-version",
    "dev": "npm-run-all --parallel api servedev lintdev",
    "servedev": "ng serve developer --host 0.0.0.0 --port 8080 --disable-host-check --proxy-config proxy.conf.json",
    "testdev": "ng test developer",
    "lintdev": "ng lint developer",
    "start": "ng serve",
    "build": "ng build",
    "builddev": "ng build developer",
    "builddev:prod": "ng build developer --prod",
    "builddev:sourcemaps": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build developer --prod --source-map",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadlessCi --watch=false",
    "testdev:ci": "ng test developer --browsers ChromeHeadlessCi --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
