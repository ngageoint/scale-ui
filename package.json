    "clipboard": "^2.0.11",
    "codemirror": "^6.0.1",
    "core-js": "^3.35.1",
    "d3": "^7.8.5",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.14.11",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "normalize.css": "^8.0.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
