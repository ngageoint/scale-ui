    "clipboard": "^2.0.4",
    "codemirror": "^5.51.0",
    "core-js": "^2.6.5",
    "d3": "^5.15.0",
    "font-awesome": "^4.7.0",
    "js-beautify": "^1.10.0",
    "lodash": "^4.17.19",
    "moment": "^2.29.2",
    "normalize.css": "^8.0.1",
